<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.leemay.mall.sys.system.mapper.SysUserMapper">
    <select id="loadUserByUsername" resultType="cn.leemay.mall.common.data.entity.system.SysUser">
        select *
        from sys_user
        where is_delete = 0
    </select>
</mapper>
